package ch1;

public class Hw_0910 {

	public static void main(String[] args) {
		int apple = 12; //1
		System.out.println(apple);
		
		int year = 2025;//2
		System.out.println(year);
		
		int money = -1000;//3
		System.out.println(money);
		
		double length = 9.8;//4
		System.out.println(length);
		
		double score = 88.5;//5
		System.out.println(score);
		
		double minusValue = -4.6;//6
		System.out.println(minusValue);
		
		String food = "떡볶이";//7
		System.out.println(food);
		
		String city = "서울";//8
		System.out.println(city);
		
		String hobby = "코딩";//9
		System.out.println(hobby);
		
		boolean hasCar = false;//10
		System.out.println(hasCar);
		
		boolean isStudent = true;//11
		System.out.println(isStudent);
		
		boolean isWeekend = false;//12
		System.out.println(isWeekend);
		
		int a=10, b=3;//1
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b);
		System.out.println(a%b);
		int sum = 12+8;//2
		int total = 20/3;//3
		int mod = 14%5;//4
		int q5= 5%5; // %는 나머지를 구하는 연산자이기에 0의 결과를 가짐
		int q6 = 5%4; // 위와 같은 연산자로 나머지 1의 결과를 가짐
		int q7 = 5%3; // 위와 같은 연산자로 나머지 2의 결과를 가짐
		int q8 = 5%2; // 위와 같은 연산자로 나머지 3의 결과를 가짐
		int q9 = 5%1; // 위와 같은 연산자로 나머지 4의 결과를 가짐
		int q10 = 5%7; // 위와 같은 연산자로 나머지 5의 결과를 가짐
		
		
		
	}

}

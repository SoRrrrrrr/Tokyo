package ch1;

/*
 	변수란 ?	
 	: 데이터를 저장하는 그릇
 	-> 다양핱 데이터를 담을 수 있음
 	
 	자료형(dataType)이란 ?
 	: 데이터 타입은 변수에 담을 값의 종류 
 */
public class Basic2 {

	public static void main(String[] args) {
		// int : 정수
		int age = 20;
		System.out.println(age);
		// double : 실수
		double height = 170.5;
		System.out.println(height);
		
		// String : 문자열 
		String name = "Kwon";
		System.out.println(name);
		
		// boolean : 참/거짓(true, false)
		boolean ispretty = true;
		System.out.println(isPretty);
		

	}

}
